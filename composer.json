{
    "name": "pantheon-upstreams/drupal-project",
    "description": "Install Drupal 9 with Composer on Pantheon.",
    "type": "project",
    "license": "MIT",
    "repositories": [
        {
            "type": "composer",
            "url": "https://packages.drupal.org/8"
        },
        {
            "type": "path",
            "url": "upstream-configuration"
        }
    ],
    "require": {
        "drupal/block_visibility_groups": "^1.4",
        "drupal/bootstrap": "^3.23",
        "drupal/color_field": "^2.4",
        "drupal/config_update": "^1.7",
        "drupal/ctools": "^3.7",
        "drupal/easy_breadcrumb": "^1.13",
        "drupal/editor_advanced_link": "^1.9",
        "drupal/entity_reference_revisions": "^1.9",
        "drupal/features": "^3.12",
        "drupal/field_group": "^3.1",
        "drupal/google_analytics": "^2.4",
        "drupal/imce": "^2.4",
        "drupal/menu_block": "^1.7",
        "drupal/menu_breadcrumb": "^1.16",
        "drupal/menu_link_attributes": "^1.2",
        "drupal/menu_trail_by_path": "^1.3",
        "drupal/metatag": "^1.11",
        "drupal/paragraphs": "^1.12",
        "drupal/pathauto": "^1.8",
        "drupal/redirect": "^1.6",
        "drupal/scheduler": "^1.4",
        "drupal/simple_sitemap": "^3.8",
        "drupal/site_settings": "^1.19",
        "drupal/smtp": "^1.0-beta4",
        "drupal/svg_image": "^1.14",
        "drupal/token": "^1.9",
        "drupal/viewsreference": "^2.0-beta2",
        "drupal/webform": "^6.0",
        "drush/drush": "^10",
        "pantheon-upstreams/upstream-configuration": "*"
    },
    "conflict": {
            "drupal/drupal": "*"
    },
    "minimum-stability": "dev",
    "prefer-stable": true,
    "extra": {
        "installer-paths": {
            "web/core": ["type:drupal-core"],
            "web/libraries/{$name}": ["type:drupal-library"],
            "web/modules/composer/{$name}": ["type:drupal-module"],
            "web/profiles/composer/{$name}": ["type:drupal-profile"],
            "web/themes/composer/{$name}": ["type:drupal-theme"],
            "drush/Commands/contrib/{$name}": ["type:drupal-drush"],
            "web/private/scripts/quicksilver/{$name}/": ["type:quicksilver-script"]
        },
        "composer-exit-on-patch-failure": true,
        "patchLevel": {
            "drupal/core": "-p2"
        },
        "drupal-scaffold": {
            "allowed-packages": [
                "pantheon-systems/drupal-integrations"
            ],
            "locations": {
                "web-root": "./web"
            },
            "file-mapping": {
                "[project-root]/.editorconfig": false,
                "[project-root]/pantheon.upstream.yml": false,
                "[project-root]/.gitattributes": false
            }
        }
    },
    "config": {
        "preferred-install": "dist",
        "sort-packages": true,
        "platform": {
            "php": "7.4"
        }
    }
}
