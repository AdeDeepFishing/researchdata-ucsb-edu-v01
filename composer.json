{
    "name": "pantheon-upstreams/drupal-project",
    "description": "Install Drupal 9 with Composer on Pantheon.",
    "type": "project",
    "license": "MIT",
    "repositories": [
        {
            "type": "composer",
            "url": "https://packages.drupal.org/8"
        },
        {
            "type": "path",
            "url": "upstream-configuration"
        },
        {
            "type": "composer",
            "url": "https://asset-packagist.org"
        }
    ],
    "require": {
        "bower-asset/jquery-simple-color": "^1.2",
        "bower-asset/lazysizes": "^5.1",
        "bower-asset/spectrum": "^1.8",
        "drupal/anchor_link": "^2.5",
        "drupal/asset_injector": "^2.7",
        "drupal/back_to_top": "^2.0",
        "drupal/block_cache": "^1.1",
        "drupal/block_field": "^1.0@RC",
        "drupal/block_visibility_groups": "^1.4",
        "drupal/bootstrap": "^3.23",
        "drupal/captcha": "^1.2",
        "drupal/cas": "^1.7",
        "drupal/color_field": "^2.4",
        "drupal/config_split": "^1.7",
        "drupal/config_update": "^1.7",
        "drupal/content_moderation_notifications": "^3.3",
        "drupal/csv_importer": "^1.11",
        "drupal/csv_serialization": "^2.0",
        "drupal/ctools": "^3.7",
        "drupal/default_content": "^2.0@alpha",
        "drupal/devel": "^4.1",
        "drupal/diff": "^1.0",
        "drupal/easy_breadcrumb": "^2.0",
        "drupal/editor_advanced_link": "^2.0",
        "drupal/embed": "^1.4",
        "drupal/entity_browser": "^2.6",
        "drupal/entity_embed": "^1.1",
        "drupal/entity_reference_revisions": "^1.9",
        "drupal/entity_usage": "^2.0@beta",
        "drupal/externalauth": "^1.4",
        "drupal/fakeobjects": "^1.1",
        "drupal/features": "^3.12",
        "drupal/feeds": "^3.0@alpha",
        "drupal/field_group": "^3.2",
        "drupal/geolocation": "^3.7",
        "drupal/google_analytics": "^3.1",
        "drupal/honeypot": "^2.0",
        "drupal/imce": "^2.4",
        "drupal/inline_entity_form": "^1.0@RC",
        "drupal/lazy": "^3.10",
        "drupal/linkit": "^6.0@beta",
        "drupal/media_entity_browser": "^2.0@alpha",
        "drupal/menu_block": "^1.7",
        "drupal/menu_breadcrumb": "^1.16",
        "drupal/menu_link_attributes": "^1.2",
        "drupal/menu_trail_by_path": "^1.3",
        "drupal/metatag": "^1.16",
        "drupal/migrate_plus": "^5.1",
        "drupal/migrate_tools": "^5.0",
        "drupal/pantheon_advanced_page_cache": "^1.2",
        "drupal/paragraphs": "^1.12",
        "drupal/pathauto": "^1.8",
        "drupal/permissions_by_term": "^3.1",
        "drupal/rabbit_hole": "^1.0@beta",
        "drupal/real_aes": "^2.3",
        "drupal/redirect": "^1.6",
        "drupal/redis": "^1.5",
        "drupal/replicate": "^1.0",
        "drupal/replicate_ui": "^1.0",
        "drupal/responsive_tables_filter": "^1.8",
        "drupal/scheduler": "^1.4",
        "drupal/simple_gmap": "^3.0",
        "drupal/simple_instagram_feed": "^3.11",
        "drupal/simple_sitemap": "^4.0@alpha",
        "drupal/site_settings": "^1.19",
        "drupal/smtp": "^1.0",
        "drupal/svg_image": "^1.15",
        "drupal/tawk_to": "^2.0",
        "drupal/toolbar_menu": "^2.2",
        "drupal/twig_tweak": "^3.1",
        "drupal/userprotect": "^1.1",
        "drupal/video_embed_field": "^2.4",
        "drupal/views_conditional": "^1.4",
        "drupal/views_data_export": "^1.0",
        "drupal/viewsreference": "^2.0@beta",
        "drupal/webform": "^6.0",
        "drupal/webform_encrypt": "^1.0@beta",
        "drupal/webform_views": "^5.0@alpha",
        "drush/drush": "^10",
        "oomphinc/composer-installers-extender": "^2.0",
        "pantheon-upstreams/upstream-configuration": "*"
    },
    "conflict": {
            "drupal/drupal": "*"
    },
    "minimum-stability": "dev",
    "prefer-stable": true,
    "extra": {
        "installer-types": ["bower-asset", "npm-asset"],
        "installer-paths": {
            "web/core": ["type:drupal-core"],
            "web/modules/contrib/{$name}": ["type:drupal-module"],
            "web/profiles/contrib/{$name}": ["type:drupal-profile"],
            "web/themes/contrib/{$name}": ["type:drupal-theme"],
            "drush/Commands/contrib/{$name}": ["type:drupal-drush"],
            "web/private/scripts/quicksilver/{$name}/": ["type:quicksilver-script"],
            "web/profiles/custom/{$name}": ["type:student_affairs"],
            "web/libraries/{$name}": ["type:drupal-library", "type:bower-asset", "type:npm-asset"]
        },
        "composer-exit-on-patch-failure": true,
        "patchLevel": {
            "drupal/core": "-p2"
        },
        "drupal-scaffold": {
            "allowed-packages": [
                "pantheon-systems/drupal-integrations"
            ],
            "locations": {
                "web-root": "./web"
            },
            "file-mapping": {
                "[project-root]/.editorconfig": false,
                "[project-root]/pantheon.upstream.yml": false,
                "[project-root]/.gitattributes": false
            }
        }
    },
    "config": {
        "preferred-install": "dist",
        "sort-packages": true,
        "platform": {
            "php": "7.4"
        }
    }
}
